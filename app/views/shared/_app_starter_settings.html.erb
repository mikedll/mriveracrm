<script language="Javascript">
  $(function() {
    if(typeof(gAppStarter) !== "undefined") {
      gAppStarter.start({
          selector: '.<%= app_class %>-gui',
          <%- if multiplicity == 'plural' %>
            modelCollectionKlass: <%= primary_model_names[:camelized_plural] %>,
            rootAppViewKlass: <%= primary_model_names[:camelized_singular] %>AppView
          <%- else %>
            modelKlass: <%= primary_model_names[:js_name_singular] %>,
            modelViewKlass: <%= primary_model_names[:js_name_singular] %>View,
            rootAppViewKlass: SingleModelAppView
          <%- end %>
          <%- if primary_model_names[:bootstrap_name] %>
            ,lazyBootstrap: function() { return __<%= primary_model_names[:bootstrap_name] %>; }
          <%- end %>
          <%- if !app_starter_params.empty? %>
            <%- app_starter_params.each do |k,v| %>
              ,<%= j(k) %>: <%= j(v) %>
            <%- end %>
          <% end %>
        })
    }
  });
</script>
