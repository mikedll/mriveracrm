!!!

%html
  %head
    %title The Mike De La Loza Company
    = csrf_meta_tag

    -# bootstrap
    = stylesheet_link_tag "//netdna.bootstrapcdn.com/twitter-bootstrap/2.3.1/css/bootstrap-combined.min.css"
    = stylesheet_link_tag :defaults
    = stylesheet_link_tag 'vendor/jquery-ui/jquery-ui'
    
    = javascript_include_tag :defaults
    = javascript_include_tag "//netdna.bootstrapcdn.com/twitter-bootstrap/2.3.1/js/bootstrap.min.js"
    = javascript_include_tag "http://www.google.com/jsapi?key=ABQIAAAA72yvhISft7fcXhZGyvHBHxRsGGQrPf-nc6BgXjkx_4DXFvH7xxRDhZbcZcLQhHVpXhIMcqUTfHQQ8g"
    = javascript_include_tag "pretty.js"
    = javascript_include_tag "vendor/underscore.js"
    = javascript_include_tag "vendor/backbone.js"
    = javascript_include_tag "generated/eventHotKeys.js"
    = javascript_include_tag "generated/main.js"
    = javascript_include_tag "vendor/jquery-ui/jquery-ui.js"

    = yield :head

  %body
    = render :partial => "shared/gain_layout"
    %div{ 'data-asset-prefix' => asset_prefix, 'id' => 'header'}

    = yield

    = render :partial => "shared/ganalytics" if Rails.env.production?
    = render :partial => "shared/login"
