!!!

%html
  %head
    %title The Mike De La Loza Company
    = csrf_meta_tag

    = stylesheet_link_tag :defaults
    = javascript_include_tag :defaults
    = javascript_include_tag "http://www.google.com/jsapi?key=ABQIAAAA72yvhISft7fcXhZGyvHBHxRsGGQrPf-nc6BgXjkx_4DXFvH7xxRDhZbcZcLQhHVpXhIMcqUTfHQQ8g"
    = javascript_include_tag "pretty.js"

    = yield :head

  %body

    = render :partial => "shared/gain_layout"

    #header

    #main.container_hook

      %h2
        The Mike De La Loza Company

      .clear

      = yield

    :javascript
      var gaJsHost = (("https:" == document.location.protocol) ? "https://ssl." : "http://www.");
      document.write(unescape("%3Cscript src='" + gaJsHost + "google-analytics.com/ga.js' type='text/javascript'%3E%3C/script%3E"));

    :javascript
      try {
      var pageTracker = _gat._getTracker("UA-8301470-1");
      pageTracker._trackPageview();
      } catch(err) {}
